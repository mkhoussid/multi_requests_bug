{
	"name": "multi-request-error",
	"version": "1.0.0",
	"description": "This README would normally document whatever steps are necessary to get your application up and running.",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production npx babel-node server.js",
		"server": "cross-env NODE_ENV=development nodemon --exec babel-node server.js",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"lint": "eslint . --ext .js",
		"install:all": "npm install && npm install --prefix client",
		"docker:build": "docker build -t multi-requests/backend . && docker build -t multi-requests/frontend ./client && docker compose up"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/eslint-parser": "^7.17.0",
		"@types/node": "^17.0.34",
		"concurrently": "^7.1.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^4.3.8",
		"nodemon": "^2.0.18"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run lint --prefix client"
		}
	},
	"dependencies": {
		"@babel/core": "^7.17.9",
		"@babel/node": "^7.16.8",
		"@babel/preset-env": "^7.16.11",
		"@fastify/cors": "^7.0.0",
		"@fastify/formbody": "^6.0.0",
		"@fastify/helmet": "^8.0.0",
		"bcryptjs": "^2.4.3",
		"chalk": "^4.1.2",
		"connection-string": "^4.3.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"fastify": "^3.28.0",
		"fastify-plugin": "^3.0.1",
		"nodemailer": "^6.7.7",
		"pg": "^8.7.3",
		"prettier": "^2.6.2",
		"winston": "^3.7.2"
	},
	"nodemonConfig": {
		"ignore": [
			"*.json"
		]
	}
}
